@article{JarvisRatliff1982,
	title = {Some Equivalent Objectives for Dynamic Network Flow Problems},
	volume = {28},
	issn = {0025-1909},
	url = {http://pubsonline.informs.org/doi/abs/10.1287/mnsc.28.1.106},
